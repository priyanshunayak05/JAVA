import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();

		long[]dp=new long[91];
		dp[0]=1;
		dp[1]=2;

		for(int i=2;i<=90;i++){
			dp[i]=dp[i-1]+dp[i-2]; // recurence relation store all that can ocuur in dp 
		}

		while(t -->0){
			int n=sc.nextInt();
			System.out.println(dp[n]);
		}

    }
}
